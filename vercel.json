{
  "version": 2,
  "routes": [
    {
      "src": "/a/(.*)",
      "dest": "https://www.google.com/$1"
    },
    {
      "src": "/b/(.*)",
      "dest": "https://www.google.com/$1"
    }
  ],
  "headers": [
    {
      "source": "/a/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Access-Control-Allow-Methods", "value": "GET,HEAD,POST,OPTIONS" },
        { "key": "Access-Control-Allow-Headers", "value": "*" }
      ]
    },
    {
      "source": "/b/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Access-Control-Allow-Methods", "value": "GET,HEAD,POST,OPTIONS" },
        { "key": "Access-Control-Allow-Headers", "value": "*" }
      ]
    }
  ]
} 