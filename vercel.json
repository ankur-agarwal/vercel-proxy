{
  "version": 2,
  "rewrites": [
    {
      "source": "/a/:path*",
      "destination": "https://www.google.com/:path*"
    },
    {
      "source": "/b/:path*",
      "destination": "https://www.google.com/:path*"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Access-Control-Allow-Methods", "value": "GET,HEAD,POST,OPTIONS" },
        { "key": "Access-Control-Allow-Headers", "value": "*" }
      ]
    }
  ]
} 